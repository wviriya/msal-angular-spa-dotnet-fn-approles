{
     "routes": [
       {
         "http_methods": [ "GET", "POST", "PUT", "DELETE" ],
         "path_prefix": "/api/todos/*",
         "policies": {
           "unauthenticated_action": "RedirectToLoginPage"
         }
       },
       {
          "http_methods": [ "HEAD" ],
          "path_prefix": "/api/todos",
          "policies": {
            "unauthenticated_action": "AllowAnonymous"
          }
       },
       {
          "http_methods": [ "GET" ],
          "path_prefix": "/api/swagger.*",
          "policies": {
            "unauthenticated_action": "AllowAnonymous"
          }
       }
     ]
}